                        "insertNewKey": "false",
                        "dumpFirstRowToParams": "false"
                    },
                    "keyColumns": {
                        "id": {
                            "@class": "KeyColumn",
                            "@attributes": {
                                "name": "id"
                            }
                        }
                    }
                }
            },
            "fields": {
                "id": {
                    "@class": "TextBoxField",
                    "@attributes": {
                        "name": "id",
                        "caption": "ID",
                        "isVisible": "true",
                        "width": "0",
                        "defaultValue": "",
                        "column": "id",
                        "readOnly": "true",
                        "notNull": "false",
                        "align": "left"
                    }
                },
                "first_name": {
                    "@class": "TextBoxField",
                    "@attributes": {
                        "name": "first_name",
                        "caption": "First Name",
                        "isVisible": "true",
                        "width": "0",
                        "defaultValue": "",
                        "column": "first_name",
                        "readOnly": "false",
                        "notNull": "false",
                        "align": "left"
                    }
                },
                "last_name": {
                    "@class": "TextBoxField",
                    "@attributes": {
                        "name": "last_name",
                        "caption": "Last Name",
                        "isVisible": "true",
                        "width": "0",
                        "defaultValue": "",
                        "column": "last_name",
