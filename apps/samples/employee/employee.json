{
    "@class": "Page",
    "@attributes": {
        "formatVer": "0.1",
        "name": "employee",
        "caption": "Employee",
        "width": "640",
        "height": "480"
    },
    "dataSources": {},
    "forms": {
        "employee": {
            "@class": "RowForm",
