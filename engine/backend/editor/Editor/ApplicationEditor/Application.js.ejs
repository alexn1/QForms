QForms.inherit(<%=application%>Controller, <%=_class%>Controller);

////////////////////////////////////////////////////////////////////////////////////////////////////
function <%=application%>Controller(model, view) {
    <%=application%>Controller.super_.call(this, model, view);
};

////////////////////////////////////////////////////////////////////////////////////////////////////
<%=application%>Controller.prototype.init = function() {
    <%=application%>Controller.super_.prototype.init.call(this);
};