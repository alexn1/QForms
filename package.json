        "@babel/plugin-transform-react-jsx": "^7.16.5",
        "@babel/preset-react": "^7.18.6",
        "@types/cookie-parser": "^1.4.3",
        "@types/express": "^4.17.14",
        "@types/express-session": "^1.17.6",
        "@types/glob": "^8.1.0",
        "@types/jest": "^29.4.0",
        "@types/mysql": "^2.15.21",
        "@types/node": "^14.18.34",
        "@types/node-fetch": "^2.6.4",
        "@types/pg": "^8.6.6",
        "@types/react": "^17.0.52",
