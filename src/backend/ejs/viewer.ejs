<!DOCTYPE html>
<html>
<head>
    <!-- <%=version%> -->
    <meta charset="utf-8"/>
    <title><%=caption%>[<%=env%>] - QForms</title>
    <!-- EventEmitter -->
    <script src="common/lib/EventEmitter/EventEmitter.min.js"></script>
    <!-- react -->
    <script src="common/lib/react/react.development.js"></script>
    <script src="common/lib/react/react-dom.development.js"></script>
    <!-- qforms -->
    <link rel="stylesheet" href="common/class/all.css"/>
    <link rel="stylesheet" href="common/jsx/all.css"/>
    <!-- viewerClassCss -->
<%viewerClassCss.forEach((filePath) => {
        %>    <link rel="stylesheet" href="<%=filePath%>"/><%='\n'%><%
    });%>
    <script src="common/class/all.js"></script>
    <script src="viewer/class/all.js"></script>
    <!-- jsx -->
    <script src="common/jsx/ReactComponent/ReactComponent.js"></script>
    <script src="common/jsx/Box/Box.js"></script>
    <script src="common/jsx/Menu/Menu.js"></script>
    <script src="common/jsx/Statusbar/Statusbar.js"></script>
    <script src="common/jsx/Tooltip/Tooltip.js"></script>
    <script src="common/jsx/DatePicker/DatePicker.js"></script>
    <script src="common/jsx/DropdownDatePicker/DropdownDatePicker.js"></script>
    <script src="common/jsx/DropdownButton/DropdownButton.js"></script>
    <script src="common/jsx/Button/Button.js"></script>
    <script src="common/jsx/TextBox/TextBox.js"></script>
    <script src="common/jsx/ComboBox/ComboBox.js"></script>
    <script src="common/jsx/Grid/Grid.js"></script>
    <script src="common/jsx/GridRow/GridRow.js"></script>
    <script src="common/jsx/GridCell/GridCell.js"></script>
    <script src="common/jsx/Modal/Modal.js"></script>
    <script src="common/jsx/TextArea/TextArea.js"></script>
    <script src="common/jsx/Tab/Tab.js"></script>


    <script src="common/jsx/_view/TableFormView/TableFormView.js"></script>
    <script src="common/jsx/_view/RowFormView/RowFormView.js"></script>
    <script src="common/jsx/_view/PageView/PageView.js"></script>
    <script src="common/jsx/_view/ApplicationView/ApplicationView.js"></script>
    <script src="common/jsx/_view/TableFormTextBoxFieldView/TableFormTextBoxFieldView.js"></script>
    <script src="common/jsx/_view/TableFormDatePickerFieldView/TableFormDatePickerFieldView.js"></script>
    <script src="common/jsx/_view/RowFormTextBoxFieldView/RowFormTextBoxFieldView.js"></script>
    <script src="common/jsx/_view/RowFormComboBoxFieldView/RowFormComboBoxFieldView.js"></script>
    <script src="common/jsx/_view/RowFormDatePickerFieldView/RowFormDatePickerFieldView.js"></script>
    <script src="common/jsx/_view/RowFormTextAreaFieldView/RowFormTextAreaFieldView.js"></script>


    <!-- app links begin -->
    <%links.forEach((link) => {
        %><link rel="stylesheet" href="<%=link%>"/><%
    });%>
    <!-- app links end -->
    <script>
    document.addEventListener('DOMContentLoaded', () => {
        const data = <%-JSON.stringify(data, null, 4)%>;
        const qforms = new QForms(data);
        qforms.init();
    });
    </script>
</head>
<body>
<%
if (debugApp === '1') {
    %><div id="root" class="debug"></div>
<%
} else {
    %><div id="root"></div><%
}
%>
</body>
</html>
