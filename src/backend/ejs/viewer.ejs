<!DOCTYPE html>
<html>
<head>
    <!-- <%=version%> -->
    <meta charset="utf-8"/>
    <title><%=caption%>[<%=env%>] - QForms</title>
    <!-- EventEmitter -->
    <script src="common/lib/EventEmitter/EventEmitter.min.js"></script>
    <!-- bluebird -->
    <script src="common/lib/bluebird/bluebird.min.js"></script>
    <!-- jquery -->
    <script src="common/lib/jquery/jquery-2.1.4.min.js"></script>
    <!-- bootstrap -->
    <link href="common/lib/bootstrap/css/bootstrap.min.css" rel="stylesheet">
    <link href="common/lib/bootstrap/css/bootstrap-theme.min.css" rel="stylesheet">
    <script src="common/lib/bootstrap/js/bootstrap.min.js"></script>
    <!-- ejs -->
    <script src="common/lib/ejs/ejs_production.js"></script>
    <!-- qforms -->
    <link rel="stylesheet" href="common/class/all.css"/>

    <!-- viewerClassCss -->
<%viewerClassCss.forEach((filePath) => {
        %>    <link rel="stylesheet" href="<%=filePath%>"/><%='\n'%><%
    });%>
    <script src="common/class/all.js"></script>
    <script src="viewer/class/all.js"></script>
    <!-- app links begin -->
    <%links.forEach((link) => {
        %><link rel="stylesheet" href="<%=link%>"/><%
    });%>
    <!-- app links end -->
    <script>
    $(document).ready(() => {
        const data = <%-JSON.stringify(data, null, 4)%>;
        new QForms(data);
        new ViewerController(data).init();
    });
    </script>
</head>
<body>
<%
if (debugApp === '1') {
    %><div id="client" class="debug"></div><%
} else {
    %><div id="client"></div><%
}
%>
</body>
</html>
