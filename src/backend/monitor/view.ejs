<!DOCTYPE html>
<html>
<head>
    <!-- <%=version%> -->
    <meta charset="utf-8"/>
    <title>QForms monitor v<%=version%></title>
    <!-- EventEmitter -->
    <script src="lib/EventEmitter/EventEmitter.min.js"></script>
    <!-- react -->
    <script src="lib/react/react.development.js"></script>
    <script src="lib/react/react-dom.development.js"></script>
    <!-- qforms css -->
    <link href="css/common-jsx.css" rel="stylesheet"/>
    <!-- qforms js -->
    <script src="js/common.js"></script>
    <script src="js/common-jsx.js"></script>
    <script id="data" type="application/json"><%-JSON.stringify(dump, null, 4)%></script>
    <script>
        document.addEventListener('DOMContentLoaded', () => {
            const data = JSON.parse(document.getElementById('data').textContent);
            Helper.createReactComponent(document.querySelector('#root'), MonitorView, {data});
        });
    </script>
</head>
<body>
    <div id="root"></div>
</body>
</html>
