class <%=page%><%=form%><%=dataSource%>Controller extends <%=_class%>Controller {

    constructor() {
        super(arguments);
    }

    select(context) {
        return super.select(context);
    }

    insert(context) {
        return super.insert(this, context);
    }

    update(context) {
        return super.update(this, context);
    }

    delete(context) {
        return super.delete(this, context);
    }

}

<%=page%><%=form%><%=dataSource%>Controller;
