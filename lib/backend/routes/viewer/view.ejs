<!DOCTYPE html>
<html>
<head>
    <!-- <%=version%> -->
    <meta charset="utf-8"/>
    <title><%=caption%> - QForms</title>
    <!-- jquery -->
    <script src="/common/lib/jquery/jquery-2.1.4.min.js"></script>
    <!-- bootstrap -->
    <link href="/common/lib/bootstrap/css/bootstrap.min.css" rel="stylesheet">
    <link href="/common/lib/bootstrap/css/bootstrap-theme.min.css" rel="stylesheet">
    <script src="/common/lib/bootstrap/js/bootstrap.min.js"></script>
    <!-- ejs -->
    <script src="/common/lib/ejs/ejs_production.js"></script>
    <!-- qforms -->
    <%commonClassCss.forEach(function (filePath) {
        %><link rel="stylesheet" href="/<%=filePath%>"/><%='\n'%><%
    });%>
    <%viewerClassCss.forEach(function (filePath) {
        %><link rel="stylesheet" href="/<%=filePath%>"/><%='\n'%><%
    });%>
    <%commonClassJs.forEach(function (filePath) {
        %><script src="/<%=filePath%>"></script><%='\n'%><%
    });%>
    <%viewerClassJs.forEach(function (filePath) {
        %><script src="/<%=filePath%>"></script><%='\n'%><%
    });%>
    <!-- app -->
    <%links.forEach(function(link) {
        %><link rel="stylesheet" href="/<%=link%>"/><%
    });%>
    <!-- end -->
    <script>
    $(document).ready(function() {
        var data = <%-JSON.stringify(data, null ,4)%>;
        QForms(data);
        new ViewerController(data).init();
    });
    </script>
</head>
<body>
<%
if (debugApp === '1') {
    %><div id="client" class="debug"></div><%
} else {
    %><div id="client"></div><%
}
%>
</body>
</html>