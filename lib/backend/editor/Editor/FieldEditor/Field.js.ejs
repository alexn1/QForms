QForms.inherit(<%=page%><%=form%><%=field%>Controller, <%=_class%>Controller);

////////////////////////////////////////////////////////////////////////////////////////////////////
function <%=page%><%=form%><%=field%>Controller() {
    <%=page%><%=form%><%=field%>Controller.super_.apply(this, arguments);
}

////////////////////////////////////////////////////////////////////////////////////////////////////
<%=page%><%=form%><%=field%>Controller.prototype.init = function() {
    <%=page%><%=form%><%=field%>Controller.super_.prototype.init.call(this);
};

////////////////////////////////////////////////////////////////////////////////////////////////////
<%=page%><%=form%><%=field%>Controller.prototype.deinit = function() {
    <%=page%><%=form%><%=field%>Controller.super_.prototype.deinit.call(this);
};

