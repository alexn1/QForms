QForms.inherits(<%=page%>Controller, <%=_class%>Controller);

////////////////////////////////////////////////////////////////////////////////////////////////////
function <%=page%>Controller() {
    <%=page%>Controller.super_.apply(this, arguments);
}

////////////////////////////////////////////////////////////////////////////////////////////////////
<%=page%>Controller.prototype.init = function() {
    <%=page%>Controller.super_.prototype.init.call(this);
};

////////////////////////////////////////////////////////////////////////////////////////////////////
<%=page%>Controller.prototype.deinit = function() {
    <%=page%>Controller.super_.prototype.deinit.call(this);
};

